{
  "name": "jobify",
  "version": "1.0.0",
  "description": "A job tracking MERN stack application.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js --ignore client",
    "client": "npm run dev --prefix client",
    "setup-app": "npm install && cd client && npm install",
    "build-client": "cd client && npm run build",
    "build-app-production": "npm install && cd client && npm install && npm run build",
    "seed-jobs": "node ./utils/jobSeeder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edriso/jobify.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/edriso/jobify/issues"
  },
  "homepage": "https://github.com/edriso/jobify#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "concurrently": "^8.2.1",
    "cookie-parser": "^1.4.6",
    "datauri": "^4.1.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.4",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
